{
  "name": "caetaria",
  "version": "1.0.0",
  "description": "Caetaria - Automatiza WhatsApp para tu negocio",
  "main": "index.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:stable": "NODE_OPTIONS='--max-old-space-size=8192 --experimental-loader tsx/esm' NEXT_TELEMETRY_DISABLED=1 next dev --turbopack --experimental-debug",
    "dev:webpack": "next dev",
    "dev:fallback": "NODE_OPTIONS='--max-old-space-size=4096' TURBOPACK=0 next dev",
    "dev:debug": "NODE_OPTIONS='--max-old-space-size=6144' DEBUG=next:* NEXT_TELEMETRY_DEBUG=1 next dev --turbopack --experimental-debug",
    "dev:clean": "npm run clean:cache && npm run dev:stable",
    "dev:memory": "NODE_OPTIONS='--max-old-space-size=12288 --gc-interval=100' UV_THREADPOOL_SIZE=16 next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "turbo:analyze": "next dev --turbopack --experimental-debug",
    "clean:cache": "npx rimraf .next node_modules/.cache .turbo",
    "clean:all": "npm run clean:cache && npx rimraf node_modules && npm install",
    "dev:windows": "set NODE_OPTIONS=--max-old-space-size=8192 && set NEXT_TELEMETRY_DISABLED=1 && next dev --turbopack --experimental-debug",
    "dev:clean:windows": "npm run clean:cache && npm run dev:windows",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ui": "jest --watch --verbose",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:component": "jest --testPathPattern=ui",
    "test:domain": "jest --testPathPattern=domain",
    "test:integration": "jest --testPathPattern=integration",
    "test:all": "npm run test && npm run test:e2e"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@next/third-parties": "^15.1.3",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@reduxjs/toolkit": "^2.8.2",
    "@sentry/nextjs": "^10.1.0",
    "@sentry/replay": "^7.116.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.52.1",
    "@types/gif.js": "^0.2.5",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "gif.js": "^0.2.0",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.542.0",
    "next": "^15.5.2",
    "next-themes": "^0.4.6",
    "posthog-js": "^1.257.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-redux": "^9.2.0",
    "rxjs": "^7.8.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.9.0",
    "@next/eslint-plugin-next": "^15.4.6",
    "@playwright/test": "^1.48.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "24.3.0",
    "@types/react": "19.1.12",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.9.0",
    "eslint-config-next": "^15.4.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "puppeteer": "^24.17.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "5.9.2"
  }
}